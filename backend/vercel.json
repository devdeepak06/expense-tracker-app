{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/auth/login",
      "destination": "https://expense-tracker-app-gurd.vercel.app/api/auth/login"
    }
  ]
}
